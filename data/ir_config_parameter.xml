<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="gpsmap.solesgps_host" model="ir.config_parameter">         
        <field name="value">http://server.solesgps.com:8016</field>
    </record>

    
    <record id="gpsmap.solesgps_user" model="ir.config_parameter"> 
        <field name="value">solesgps</field>
    </record>
    <record id="gpsmap.solesgps_pass" model="ir.config_parameter"> 
        <field name="value">admin</field>
    </record>
    <record id="gpsmap.solesgps_db" model="ir.config_parameter"> 
        <field name="value">produccion</field>
    </record>
    
    <record id="solesgps_sync_devices" model="ir.config_parameter">
        <field name="key">gpsmap.sync_devices</field>
        <field name="value" eval="True"/>
    </record>    

</odoo>




